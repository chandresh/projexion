<div id="content">
    <% form_for @project do |f| %>
        <%= f.error_messages %>

        <%= f.label :name %><br />
        <%= f.text_field :name %><br />

        <%= f.label :code %><br />
        <%= f.text_field :code %><br />

        <%= f.label :vision %><br/>
        <%= f.text_field :vision %><br/>

        <%= f.submit 'Save' %>
    <% end %>
</div>

